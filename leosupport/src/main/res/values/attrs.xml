<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ProportionView">
        <attr name="x" format="integer" />
        <attr name="y" format="integer" />
        <attr name="maxHeight" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="TopBar">
        <attr name="heightDP" format="dimension" />
        <attr name="showStatusBar" format="boolean" />
        <attr name="statusBarColor" format="color" />
        <!--返回键-->
        <attr name="backImage" format="reference" />
        <attr name="backPaddingDP" format="dimension" />
        <!--标题-->
        <attr name="titleText" format="string" />
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
        <!--文字菜单-->
        <attr name="menuTextPaddingDP" format="dimension" />
        <attr name="menuText" format="string" />
        <attr name="menuTextSize" format="dimension" />
        <attr name="menuTextColor" format="color" />
        <!--图片菜单-->
        <attr name="menuImage" format="reference" />
        <attr name="menuImagePaddingDP" format="dimension" />
    </declare-styleable>

</resources>
